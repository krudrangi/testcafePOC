{
  "name": "testcafedemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "tsc objectRepo/*/*.ts",
    "chromeSingle": "testcafe chrome tests/large/loginTestPO.js --env=Test --reporter html:reports/chromeResulsts.html --video artifacts/videos",
    "chromeAll": "testcafe chrome tests/large/*.js --env=Test --reporter html:reports/chromeResulsts.html --video artifacts/videos",
    "firefoxAll": "testcafe firefox tests/large/*.js --env=Test --reporter html:reports/firefoxResulsts.html --video artifacts/videos",
    "safariAll": "testcafe safari tests/large/*.js --env=Test --reporter html:reports/safariResulsts.html --video artifacts/videos",
    "edgeAll": "testcafe edge tests/large/*.js --env=Test --reporter html:reports/edgeResulsts.html --video artifacts/videos",
    "sauceChrome": "testcafe \"saucelabs:Chrome@latest:macOS 10.12\" 'tests/large/*.js' -c 3",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
