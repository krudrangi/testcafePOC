  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "cookies": "^0.8.0",
    "testcafe": "^1.12.0",
    "testcafe-browser-provider-saucelabs": "^1.8.0",
    "testcafe-reporter-html": "^1.4.6"
  },
  "devDependencies": {
    "@types/cucumber": "^7.0.0",
    "cucumber": "^7.0.0-rc.0"
  }
}